{
  "name": "@slimoro/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "nx run-many -t lint --all",
    "lint:fix": "nx run-many -t lint --all -- --fix",
    "typecheck": "nx run-many -t typecheck --all || tsc -b -v",
    "build": "nx run-many -t build --all",
    "build:check": "nx run-many -t build --all --output-style=stream",
    "test": "nx run-many -t test --all --skip-nx-cache --output-style=stream -- --run --reporter verbose",
    "test:watch": "nx run-many -t test --all --output-style=stream -- --watch --reporter verbose",
    "test:affected": "nx affected -t test --output-style=stream -- --reporter verbose",
    "api:dev": "nx serve api",
    "api:build": "nx build api",
    "api:test": "nx test api",
    "api:lint": "nx lint api"
  },
  "private": true,
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250404.0",
    "@eslint/js": "^9.8.0",
    "@expo/cli": "~0.24.14",
    "@naxodev/nx-cloudflare": "^5.0.2",
    "@nx/esbuild": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/expo": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/node": "21.5.2",
    "@nx/vite": "21.5.2",
    "@nx/web": "21.5.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "jiti": "2.4.2",
    "metro-config": "~0.82.4",
    "metro-resolver": "~0.82.4",
    "nx": "21.5.2",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.26.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "expo": "~53.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3"
  }
}
