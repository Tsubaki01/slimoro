{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobile",
  "projectType": "application",
  "tags": [],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start",
        "cwd": "apps/mobile"
      }
    },
    "run-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --android",
        "cwd": "apps/mobile"
      }
    },
    "run-ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --ios",
        "cwd": "apps/mobile"
      }
    },
    "web": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo start --web",
        "cwd": "apps/mobile"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/mobile/**/*.{ts,tsx,js,jsx,cjs,mjs,cts,mts}",
          "apps/mobile/eslint.config.js"
        ]
      }
    },
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "expo prebuild",
        "cwd": "apps/mobile"
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm install",
        "cwd": "apps/mobile"
      }
    }
  }
}
